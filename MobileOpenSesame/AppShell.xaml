    Title="MobileOpenSesame">

    <ShellContent
        Title="Mobile OpenSesame"
        FlexLayout.AlignSelf="Stretch"
        FlexLayout.Basis="Auto"
        ContentTemplate="{DataTemplate views:LoginDataPage}"
